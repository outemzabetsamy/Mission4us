<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-4.6.xsd">
    <!--
        Added the constraints for entity Attachment.
    -->
    <changeSet id="20230214221139-2" author="jhipster">

        <addForeignKeyConstraint baseColumnNames="client_id"
                                 baseTableName="attachment"
                                 constraintName="fk_attachment__client_id"
                                 referencedColumnNames="id"
                                 referencedTableName="client"/>

        <addForeignKeyConstraint baseColumnNames="provider_id"
                                 baseTableName="attachment"
                                 constraintName="fk_attachment__provider_id"
                                 referencedColumnNames="id"
                                 referencedTableName="provider"/>
    </changeSet>
</databaseChangeLog>
